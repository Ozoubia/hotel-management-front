<UserControl x:Class="hotel_management_front.toolboxUserControl.fournisseurUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:hotel_management_front.toolboxUserControl"
             mc:Ignorable="d" 
             d:DesignHeight="85" d:DesignWidth="800">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <Button x:Name="fournisseurListBtn" Click="fournisseurListBtn_Click" Margin="20 0 10 0 " >Liste des fournisseur</Button>
            <Button x:Name="arrivageBtn" Click="arrivageBtn_Click" Margin="20 0 10 0" >Nouvelle Arrivage</Button>
        </StackPanel>
    </Grid>
</UserControl>
