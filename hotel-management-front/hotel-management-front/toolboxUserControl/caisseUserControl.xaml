<UserControl x:Class="hotel_management_front.toolboxUserControl.caisseUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:hotel_management_front.toolboxUserControl"
             mc:Ignorable="d" 
             d:DesignHeight="85" d:DesignWidth="600">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <Button x:Name="caissePrincipaleBtn" Margin="20 0 10 0 " Click="caissePrincipaleBtn_Click">Caisse Principale</Button>
            <Button x:Name="caissePersonnelBtn" Margin="20 0 10 0" Click="caissePersonnelBtn_Click">Caisse personnel</Button>
            <Button x:Name="bonqueBtn" Margin="20 0 10 0 " Click="bonqueBtn_Click" >Bonque </Button>
        </StackPanel>
    </Grid>
</UserControl>
