<UserControl x:Class="hotel_management_front.toolboxUserControl.StockUserControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:hotel_management_front.toolboxUserControl"
             mc:Ignorable="d" 
             d:DesignHeight="85" d:DesignWidth="600">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <Button x:Name="StockGenerale"  Margin="10 0 10 0" Click="StockGenerale_Click">Stock generale</Button>
            <Button x:Name="Cuisine" Margin="20 0 10 0" Content="Cuisine" Click="Cuisine_Click"/>
            <Button x:Name="equipement" Margin="10 0 10 0" Click="equipement_Click">équipement</Button>
        </StackPanel>
    </Grid>
</UserControl>
