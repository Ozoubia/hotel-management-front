             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:hotel_management_front.toolboxUserControl"
             mc:Ignorable="d" 
             d:DesignHeight="85" d:DesignWidth="600">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <Button x:Name="planingBtn" Margin="20 0 10 0" Click="planingBtn_Click">Planing</Button>
            <Button x:Name="statBtn" Margin="10 0 10 0" Click="statBtn_Click">Statistiques</Button>
            
        </StackPanel>
    </Grid>
</UserControl>
